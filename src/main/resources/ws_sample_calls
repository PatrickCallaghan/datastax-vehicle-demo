All calls are @POST


@Path("/area/vehicles/readings/latest")
Example JSON request:
{
    "area": {
        "polygon": {
            "geoPoints": [
                {
                    "lat": 48.736989,
                    "lng": 10.271339
                },
                {
                    "lat": 48.067576,
                    "lng": 11.60903
                },
                {
                    "lat": 48.774243,
                    "lng": 12.91312
                },
                {
                    "lat": 49.595759,
                    "lng": 11.123788
                },
                {
                    "lat": 48.736989,
                    "lng": 10.271339
                }
            ]
        },
        "circle": null
    },
    "timeframe": null,
    "measurementsRequired": false,
    "order": null,
    "filter": null
}


@Path("/area/vehicles/readings/history")
Example JSON request:
{
    "area": {
        "polygon": {
            "geoPoints": [
                {
                    "lat": 48.736989,
                    "lng": 10.271339
                },
                {
                    "lat": 48.067576,
                    "lng": 11.60903
                },
                {
                    "lat": 48.774243,
                    "lng": 12.91312
                },
                {
                    "lat": 49.595759,
                    "lng": 11.123788
                },
                {
                    "lat": 48.736989,
                    "lng": 10.271339
                }
            ]
        },
        "circle": null
    },
    "timeframe": {
        "startDate": "2018/05/10 01:10:55",
        "endDate": "2018/05/31 23:14:12"
    },
    "measurementsRequired": false,
    "order": {
        "ascending": false
    },
    "filter": null
}


@Path("/vehicle/readings/latest")
Example JSON request:
{
    "vehicleId": "989",
    "area": {
        "polygon": {
            "geoPoints": [
                {
                    "lat": 48.736989,
                    "lng": 10.271339
                },
                {
                    "lat": 48.067576,
                    "lng": 11.60903
                },
                {
                    "lat": 48.774243,
                    "lng": 12.91312
                },
                {
                    "lat": 49.595759,
                    "lng": 11.123788
                },
                {
                    "lat": 48.736989,
                    "lng": 10.271339
                }
            ]
        },
        "circle": null
    },
    "timeframe": {
        "startDate": "2018/05/10 01:10:55",
        "endDate": "2018/05/31 23:14:12"
    },
    "measurementsRequired": false,
    "order": {
        "ascending": false
    },
    "filter": null
}


